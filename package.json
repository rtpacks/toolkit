{
  "name": "@rtpackx/monorepo",
  "type": "module",
  "version": "0.0.4",
  "private": true,
  "description": "实用工具",
  "packageManager": "pnpm@9.7.0",
  "author": "MF <https://github.com/rtpacks>",
  "license": "MIT",
  "homepage": "https://github.com/rtpacks/toolkit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rtpacks/toolkit.git"
  },
  "bugs": {
    "url": "https://github.com/rtpacks/toolkit/issues"
  },
  "keywords": [
    "vue",
    "react",
    "composition"
  ],
  "scripts": {
    "test": "pnpm install && vitest --coverage",
    "test-coverage": "pnpm install && vitest --run",
    "docs:dev": "vitepress dev packages",
    "docs:preview": "vitepress preview packages",
    "docs:build": "vitepress build packages",
    "build": "pnpm install && tsx scripts/build.ts",
    "build:dts": "pnpm install && tsx scripts/build-dts.ts",
    "build:doc": "pnpm install && tsx scripts/build-dts.ts && tsx scripts/build-doc.ts",
    "release": "pnpm changelog && tsx scripts/release.ts",
    "push": "git push && git push --tags",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "lint-staged": "npx lint-staged",
    "changelog": "npx conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "simple-git-hooks"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.9.0",
    "@microsoft/api-documenter": "^7.25.11",
    "@microsoft/api-extractor": "^7.47.6",
    "@rollup/pluginutils": "^5.1.0",
    "@shikijs/vitepress-twoslash": "^1.12.1",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.2.0",
    "@types/rollup-plugin-progress": "^1.1.5",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^10.11.0",
    "bumpp": "^9.4.1",
    "change-case": "^5.4.4",
    "consola": "^3.2.3",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "is-in-ci": "^1.0.0",
    "js-base64": "^3.7.7",
    "jsdom": "^24.1.1",
    "lint-staged": "^15.2.7",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "nanoid": "^5.0.7",
    "picocolors": "^1.0.1",
    "prettier": "^3.3.3",
    "rollup": "^4.19.2",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "simple-git": "^3.25.0",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.16.5",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.1.0",
    "unplugin-auto-import": "^0.18.2",
    "vite": "^5.3.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-dts": "^4.0.3",
    "vite-plugin-imagemin": "^0.6.1",
    "vitepress": "^1.3.2",
    "vitest": "^2.0.5",
    "vue": "^3.4.35",
    "vue-demi": "^0.14.10",
    "vue-router": "^4.4.3",
    "vue-tsc": "^2.0.29"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue,json}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "npx tsx scripts/verify-commit.ts"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
