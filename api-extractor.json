{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "mainEntryPointFilePath": "<scope>/dist/index.d.ts",
  "bundledPackages": [],
  "compiler": {
    "tsconfigFilePath": "tsconfig.json"
  },
  "apiReport": {
    "enabled": false,
    "reportFolder": "<scope>/",
    "reportFileName": "index.api.md"
  },
  "docModel": {
    "enabled": true,
    "apiJsonFilePath": "<scope>/dist/index.api.json"
  },
  "dtsRollup": {
    "enabled": false
  },
  "tsdocMetadata": {},
  "messages": {
    "compilerMessageReporting": {
      "default": {
        "logLevel": "warning"
      }
    },
    "extractorMessageReporting": {
      "default": {
        "logLevel": "warning"
      },
      "ae-wrong-input-file-type": {
        "logLevel": "warning"
      }
    },
    "tsdocMessageReporting": {
      "default": {
        "logLevel": "warning"
      }
    }
  }
}
